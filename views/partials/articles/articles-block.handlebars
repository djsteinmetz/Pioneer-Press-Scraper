{{#each articles}}
<div class="card article" data-id={{_id}} onclick="openNav()">
    <img class="card-img-top" src="{{imgUrl}}" alt="Article Image">
    <div class="card-body">
        <h3 class="card-title">{{title}}</h3>
        <p class="card-text">{{byLine}}</p>
        <a href="{{link}}" class="btn btn-dark thin" target="_blank">Full Article</a>
    </div>
</div>
{{/each}}